tweet = input('Enter abbreviation from tweet:\n')
Abbrev = {
    'LOL': 'laughing out loud',
    'BFN': 'bye for now',
    'FTW': 'for the win',
    'IRL': 'in real life',
    'WTF': 'whens the foxtrot',
    'THX': 'thank you',
    'TTYL': 'talk to you later',
    'BRB': 'be right back',
    'AFK': 'away from keyboard'
   }
if tweet in Abbrev:
    print(tweet,'=', Abbrev[tweet])
else:
    print("Sorry, don't know that one")

